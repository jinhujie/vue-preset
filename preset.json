{
  "vueVersion": "3",
  "packageManager": "yarn",
  "usetaobaoRegistry": true,
  "plugins": {
    "@vue/cli-plugin-typescript": {
      "prompts": false
    },
    "@vue/cli-plugin-eslint": {
      "prompts": false,
      "config": "prettier",
      "lintOn": [
        "save"
      ],
      "@vue/test-utils": {
        "prompts": false
      }
    },
    "@vue/cli-plugin-unit-jest": {
      "prompts": false
    },
    "@vue/cli-plugin-babel": {
      "prompts": false
    }
  }
}